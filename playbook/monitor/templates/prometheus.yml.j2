global:
  scrape_interval: 15s

scrape_configs:
  - job_name: "prometheus"
    static_configs:
      - targets: ["localhost:9090"]

  - job_name: "Node Server"
    static_configs:
      - targets:
          - "192.168.1.100:9100"

# Tambahkan VM lain jika diperlukan
  - job_name: "VM Utama"
    static_configs:
      - targets:
          - "192.168.1.6:9100"  
